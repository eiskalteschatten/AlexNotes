{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "pretty": true,
        "sourceMap": false,
        "target": "es6",
        "outDir": "../dist",
        "baseUrl": "./src",
        "resolveJsonModule": true
    },
    "types": [
        "webpack-env",
        "vuetify"
    ],
    "include": [
        "./src/**/*.ts",
        "./src/**/*.vue"
    ],
    "exclude": [
        "node_modules"
    ]
}
